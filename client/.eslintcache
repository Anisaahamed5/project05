t { Link } from 'react-router-dom';\n\nimport './login.css';\n\nclass Login extends Component {\n    state = {\n        username: \"\",\n        password: \"\",\n        errors: []\n    }\n\n    onChange(data) {\n        this.setState({...data, errors: []});\n    }\n\n    renderErrors() {\n        return <ul className=\"text-danger pl-3\">\n            {\n                this.state.errors.map((error) => {\n                    return <li>{error}</li>\n                })\n            }\n        </ul>;\n    }\n\n    // Submit the data to the API\n    async handleSubmit(e) {\n        e.preventDefault();\n        \n        const data = {\n            username: this.state.username, \n            password: this.state.password\n        }\n\n        const response = await fetch('/api/login', {\n            method: 'POST',\n            headers: {\n                'Content-Type': 'application/json',\n            },\n            body: JSON.stringify(data),\n        }).then((res) => {\n            return res.json();\n        });  \n\n        if(response.success == true) {\n            // Login succeeded\n            this.props.login(response.user);\n        } else {\n            // Login failed\n            this.setState({\n                errors: [\"Username or password is incorrect\"]\n            });\n        }\n    }\n\n    render() {\n        return (\n            <div id=\"login\" className=\"container\">\n                { this.renderErrors() }\n                <form>\n                    <div className=\"form-group\">\n                        <label htmlFor=\"username\">Username</label>\n                        <input type=\"text\" className=\"form-control\" id=\"username\" placeholder=\"Enter username\" value={this.state.username} onChange={(e) => this.onChange({username: e.target.value})}/>\n                    </div>\n                    <div className=\"form-group\">\n                        <label htmlFor=\"password\">Password</label>\n                        <input type=\"password\" className=\"form-control\" id=\"password\" placeholder=\"Password\" value={this.state.password} onChange={(e) => this.onChange({password: e.target.value})}/>\n                    </div>\n                    <button type=\"submit\" className=\"btn btn-primary mr-2\" onClick={(e) => this.handleSubmit(e)}>Login</button>\n                    <Link to=\"/register\">\n                        <button type=\"button\" className=\"btn btn-secondary\">Register</button>\n                    </Link>\n                </form>\n            </div>\n        );\n    }\n}\n\nexport default Login;","/Users/anisaahamed/Documents/Projects/project05/client/src/pages/main/index.js",["59","60"],"/Users/anisaahamed/Documents/Projects/project05/client/src/pages/answer_question/index.js",[],"/Users/anisaahamed/Documents/Projects/project05/client/src/pages/registration/index.js",["61","62","63"],"import React, { Component } from 'react';\n\nimport './registration.css';\n\nclass Registration extends Component {\n    state = {\n        username: \"\",\n        password: \"\",\n        password2: \"\",\n        errors: []\n    }\n\n    onChange(data) {\n        this.setState({...data, errors: []});\n    }\n\n    renderErrors() {\n        return <ul className=\"text-danger pl-3\">\n            {\n                this.state.errors.map((error) => {\n                    return <li>{error}</li>\n                })\n            }\n        </ul>;\n    }\n\n    handleSubmit() {\n        let errors = [];\n\n        if(this.state.password != this.state.password2) {\n            errors.push(\"Passwords do not match\")\n        }\n\n        // Other validity checks\n        //\n\n        this.setState({\n            errors\n        });\n\n        if(errors.length < 1) {\n            this.submit();\n        }\n    }\n\n    // Submit the data to the API\n    async submit() {\n        const data = {\n            username: this.state.username, \n            password: this.state.password\n       